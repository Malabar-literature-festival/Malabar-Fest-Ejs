        <div class="registration-title">
          <h1>Volunteer Registration</h1>
        </div>
      </div>
    </div>
    <div class="register-table"
      style="background-image: url('assets/images/RegisterUnion.png'); background-size: cover; background-position: center center; margin-top: 60px; width: 100%; min-height: 818px; display: flex; justify-content: center; align-items: center;">
