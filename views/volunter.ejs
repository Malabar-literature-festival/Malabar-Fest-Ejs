                        <option value="Fine Arts">Fine Arts</option>
                        <option value="Architecture">Architecture</option>
                        <option value="Film & Music">Film & Music</option>
                        <option value="Health">Health</option>
                        <option value="Spirituality">Spirituality</option>
                        <option value="Sufism">Sufism</option>
                        <option value="Sports & Fitness">Sports & Fitness</option>
                        <option value="Science & Tech">Science & Tech</option>
                        <option value="Charity & Causes">Charity & Causes</option>
                        <option value="Family & Education">Family & Education</option>
                        <option value="Social Media">Social Media</option>
                        <option value="Social Activism">Social Activism</option>
                      </select>
                    </div>
                  </div>

                  <div class="col-md-6">
                    <div class="form-group">
                      <label for="day">Select Day:</label>
                      <input type="date" id="day" name="day" class="form-control" required>
                    </div>

                  </div>
                </div>
                <div class="row register-amount-btn">
                  <div class=" col-12 d-flex justify-content-center">
                    <button type="submit" class="btn btn-primary" style="border-radius: 20px;">Register
                    </button>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>


      </div>
    </div>
  </div>

